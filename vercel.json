{
  "rewrites": [
    {
      "source": "/((?!api|_next|assets|favicon.ico).*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "markg.dev" }],
      "destination": "https://markgarcia.dev/$1",
      "permanent": true
    }
  ]
}
